{
    "name": "cacophony-types",
    "version": "1.0.0",
    "scripts": {
        "generate-schemas": "./node_modules/.bin/tsc && node build-schemas.js",
        "lint": "./node_modules/.bin/eslint ./ --ext .ts && (npx prettier --check './**/*.ts' || (echo \"Please run 'npm run lint:fix' to clean up formatting of files.\"; false))",
        "lint:fix": "./node_modules/.bin/eslint ./ --fix --ext .ts && npx prettier --write './**/*.ts'"
    },
    "dependencies": {
        "recursive-readdir": "^2.2.2",
        "ts-json-schema-generator": "^1.2.0"
    },
    "devDependencies": {
        "@typescript-eslint/eslint-plugin": "^5.59.6",
        "@typescript-eslint/parser": "^5.59.6",
        "eslint": "^8.40.0",
        "eslint-config-prettier": "^8.8.0",
        "eslint-utils": "^3.0.0",
        "prettier": "^2.8.8",
        "typescript": "4.9.5",
        "@types/recursive-readdir": "^2.2.0",
        "@tsconfig/node16": "^1.0.4"
    }
}
